<?xml version="1.0" encoding="UTF-8"?>
<web-app 
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
		xmlns="http://java.sun.com/xml/ns/javaee" 
		xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" 
		id="WebApp_ID" 
		version="2.5"
>
	<display-name>univer</display-name>
	<welcome-file-list>
		<welcome-file>/hello</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>hello</servlet-name>
		<servlet-class>students.web.HelloWorldServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>hello</servlet-name>
		<url-pattern>/hello</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>testjdbc</servlet-name>
		<servlet-class>students.web.TestJDBCServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>testjdbc</servlet-name>
		<url-pattern>/testjdbc</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>testjdbcanswer</servlet-name>
		<servlet-class>students.web.TestJDBCAnswerServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>testjdbcanswer</servlet-name>
		<url-pattern>/testjdbcanswer</url-pattern>
	</servlet-mapping>
</web-app>
